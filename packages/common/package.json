{
    "name": "@tsai-platform/common",
    "version": "0.0.1",
    "description": "Tsailab library common",
    "author": "tsailab",
    "license": "LICENSE",
    "keywords": [
        "tsai-platform",
        "common"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "build": "pnpm clean && pnpm build:pkg",
        "build:pkg": "tsc --build --verbose --force tsconfig.pkg.json",
        "clean": "rm -rf dist/*",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,packages,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@ingestkorea/util-jsonwebtoken": "1.1.1",
        "@nestjs/common": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "bcrypt": "5.1.1",
        "class-validator": "0.14.1",
        "date-fns": "4.1.0",
        "eckey-utils": "0.7.14",
        "iterare": "1.2.1",
        "nanoid": "3.3.8",
        "tslib": "2.8.1",
        "typescript": "^5.1.3"
    },
    "peerDependencies": {
        "@ingestkorea/util-jsonwebtoken": "1.1.1",
        "@nestjs/common": "^9.4.3 || ^10.0.0",
        "@nestjs/testing": "^9.4.3 || ^10.0.0",
        "bcrypt": "5.1.1",
        "class-validator": "0.14.1",
        "date-fns": "4.1.0",
        "eckey-utils": "0.7.14",
        "iterare": "1.2.1",
        "nanoid": "3.3.8",
        "tslib": "2.8.1",
        "typescript": "^4.1.6 || ^5.1.3"
    },
    "peerDependenciesMeta": {
        "nanoid": {
            "optional": false
        }
    }
}